meta {
  name: Login
  type: http
  seq: 1
}

get {
  url: https://groups.io/api/v1/login?email={{email}}&password={{password}}
  body: json
  auth: none
}

params:query {
  email: {{email}}
  password: {{password}}
}

headers {
  Content-Type: application/json
  Accept: application/json
  Authorization: Bearer {{token}}
}
